<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <PackageLicenseFile></PackageLicenseFile>
    <Authors>CanhHungIT</Authors>
    <Description>Generic Repository Pattern and Unit Of Work for C# .Net Core</Description>
    <Copyright>Copyright Â©2020 CanhHungIT</Copyright>
    <PackageProjectUrl>https://github.com/canhhungit/MiniUnitOfWork</PackageProjectUrl>
    <RepositoryUrl>https://github.com/canhhungit/MiniUnitOfWork</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>GenericRepositoryPattern UnitOfWorkPattern .NetCore</PackageTags>
    <PackageReleaseNotes>Update Readme and add function</PackageReleaseNotes>
    <NeutralLanguage>en</NeutralLanguage>
    <Version>1.4.0</Version>
    <PackageIcon>ico.jpg</PackageIcon>
    <PackageIconUrl />
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="IRepositoryAsync.cs" />
    <Compile Remove="IRepositoryReadOnly.cs" />
    <Compile Remove="RepositoryAsync.cs" />
    <Compile Remove="RepositoryReadOnly.cs" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.EntityFrameworkCore" Version="2.1.3" />
    <PackageReference Include="Microsoft.EntityFrameworkCore.AutoHistory" Version="2.1.0" />
    <PackageReference Include="Microsoft.EntityFrameworkCore.Relational" Version="2.1.3" />
    <PackageReference Include="System.Linq" Version="4.3.0" />
  </ItemGroup>

  <ItemGroup>
    <None Include="ico.jpg">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
