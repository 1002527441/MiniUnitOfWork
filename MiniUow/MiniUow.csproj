<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <Authors>CanhHungIT</Authors>
    <Description>A library support repository, unit of work patterns</Description>
    <Copyright>Copyright Â©2020 CanhHungIT</Copyright>
    <PackageProjectUrl>https://github.com/canhhungit/MiniUnitOfWork</PackageProjectUrl>
    <RepositoryUrl>https://github.com/canhhungit/MiniUnitOfWork</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>generic repository</PackageTags>
    <PackageReleaseNotes>Update return T class when update</PackageReleaseNotes>
    <NeutralLanguage>en</NeutralLanguage>
    <Version>1.3.0</Version>
    <PackageIcon>ico.jpeg</PackageIcon>
    <PackageIconUrl />
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="IRepositoryAsync.cs" />
    <Compile Remove="IRepositoryReadOnly.cs" />
    <Compile Remove="RepositoryAsync.cs" />
    <Compile Remove="RepositoryReadOnly.cs" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.EntityFrameworkCore" Version="2.1.3" />
    <PackageReference Include="Microsoft.EntityFrameworkCore.AutoHistory" Version="2.1.0" />
    <PackageReference Include="Microsoft.EntityFrameworkCore.Relational" Version="2.1.3" />
    <PackageReference Include="System.Linq" Version="4.3.0" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
    <None Include="ico.jpeg">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
